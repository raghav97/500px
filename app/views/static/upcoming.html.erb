<% require 'json' %>

<% images = [] %>

<% response = F00px.get('photos?feature=upcoming') %>
<% a = response.body %>
<% hash = JSON.parse(a) %>
<% hash["photos"].each do |e| %>
  <% images.push(e["image_url"]) %>
<% end %>

<% images.each do |e| %>
  <div class=""><img class="image" src= "<%= e %>" />
<% end %>

<%= render 'paginate' %>
