<% require 'json' %>

<% images = [] %>

<% response = F00px.get('photos?feature=fresh_yesterday') %>
<% a = response.body %>
<% hash = JSON.parse(a) %>
<% hash["photos"].each do |e| %>
  <% images.push(e["image_url"]) %>
<% end %>

<% images.each do |e| %>
  <div class=""><img class="image" src= "<%= e %>" />
<% end %>

<div class = 'col-md-12 col-sm-12 col-xs-12'>
  <div class = "center">
    <%= render 'paginate' %>
  </div>
</div>